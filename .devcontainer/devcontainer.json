{
	"name": "node-dev",
	"image": "docker.io/library/node:24-trixie",
	"workspaceFolder": "/workspace",
	"remoteUser": "node",
	"updateRemoteUserUID": false,
	"mounts": [ "source=${localWorkspaceFolder},target=/workspace,type=bind" ],
	"customizations": {
		"vscode": {
			"extensions": [
				"esbenp.prettier-vscode"
			]
		}
	}
}
