{
    "name": "@0xmax42/n8n-nodes-tanss-api",
    "version": "0.2.0-pre.0",
    "description": "n8n community node to use the TANSS API",
    "license": "MIT",
    "homepage": "https://github.com/0xMax42/n8n-nodes-tanss-api",
    "keywords": [
        "n8n-community-node-package",
        "n8n-nodes-tanss-api",
        "Tanss"
    ],
    "author": {
        "name": "0xMax42",
        "email": "git@0xmax42.io"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/0xMax42/n8n-nodes-tanss-api.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:ci": "jest --ci",
        "test:one": "jest -t",
        "test:coverage": "jest --coverage",
        "test:sum": "jest --silent --reporters=./clean-tests-reporter.js"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/TanssApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/tanss/Tanss.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.9.1",
        "eslint": "9.32.0",
        "jest": "^30.2.0",
        "jest-summary-reporter": "^0.0.2",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "ts-jest": "^29.4.6",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
